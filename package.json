{
  "name": "optics-ts",
  "version": "1.1.0-pre.3",
  "description": "Type-safe, ergonomic, polymorphic optics for TypeScript",
  "repository": "https://github.com/akheron/optics-ts",
  "author": "Petri Lehtinen <petri@digip.org>",
  "license": "MIT",
  "keywords": [
    "optics",
    "lens",
    "prism",
    "traveral",
    "isomorphism",
    "immutable",
    "functional-programming"
  ],
  "main": "./dist/lib/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/lib/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
    "test": "jest",
    "prepublishOnly": "yarn run build",
    "doctoc": "doctoc README.md --title '## Table of Contents' && prettier --write README.md",
    "generate-index": "ts-node ./scripts/generate-index.ts > src/index.ts && prettier --write src/index.ts"
  },
  "devDependencies": {
    "@babel/types": "^7.9.0",
    "@types/jest": "^25.1.4",
    "doctoc": "^1.4.0",
    "jest": "^25.1.0",
    "prettier": "^2.0.2",
    "ts-jest": "^25.2.1",
    "ts-node": "^8.8.1",
    "typescript": "^3.8.3"
  }
}
